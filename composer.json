    "type": "library",
    "require": {
        "php": "^7.0",
        "mledoze/countries": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "php-coveralls/php-coveralls": "^2.0",
        "roave/security-advisories": "dev-master"
    },
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Jordan Hall",
            "email": "jordan@hall05.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "DivineOmega\\Countries\\": "src/"
        }
